{    
  "routes": {
      "/login/*": "index.html",
      "/promise/*": "index2.html",
      "/fetch/*": "index3.html"
  },
  "headers": {
    "/fetch/*": {
      "Access-Control-Allow-Origin": "*"  
    },
    "/login/*": {
      "Content-Type": "text/plain; charset=UTF-8",
      "Access-Control-Allow-Origin": "*"
    },
    "/promise/*": {
      "Access-Control-Allow-Origin": "*"
    }
  }
}
